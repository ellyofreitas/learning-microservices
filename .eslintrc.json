{
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["warn"],
    "no-console": [
      "error",
        {
            "allow": ["info", "error"]
        }
    ]
  }
}
